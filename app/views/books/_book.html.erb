<div id="<%= dom_id book %>">
  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Subject:</strong>
    <%= book.subject %>
  </p>

  <p>
    <strong>Temp:</strong>
    <%= book.temp %>
  </p>

</div>
